int	ft_strcmp(char *s1, char *s2)
{
	int i;
	int b;

	i = 0;
	while (s1[i] || s2[i])
	{
		if (s1[i] != s2[i])
		{
			b = s1[i] - s2[i];
			return (b);
			i++;
		}
	}
	return (0);
}
